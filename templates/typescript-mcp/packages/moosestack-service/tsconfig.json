{
  "compilerOptions": {
    "outDir": "dist",
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2022",
    "plugins": [
      {
        "transform": "./node_modules/@514labs/moose-lib/dist/compilerPlugin.js",
        "transformProgram": true
      },
      {
        "transform": "typia/lib/transform"
      }
    ],
    "strictNullChecks": true
  }
}
